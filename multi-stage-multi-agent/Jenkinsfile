pipeline {
  agent any
  stages {
    stage('git checkout') {
      steps {
        git branch: 'main', url: 'https://github.com/manikanta513/Jenkins-Zero-To-Hero.git'
      }
    }
    stage('Build Docker Image') {
      steps {
        dir('multi-stage-multi-agent') {
          sh 'docker build -t manikanta-test-java .'
          sh 'docker run manikanta-test-java'
        }
      }
    }
  }
}
